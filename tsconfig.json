{
  "compilerOptions": {
    "target": "ES5",
    "inlineSources": true,
    "module": "system",
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "sourceMap": true,
    "noResolve": false,
    "baseUrl": "./",
    "paths": {
      "angular2-redux-util": [
        "jspm_packages/npm/angular2-redux-util@0.8.64"
      ],
      "angular2-google-maps/core": [
        "jspm_packages/npm/angular2-google-maps@0.10.0/core"
      ],
      "@angular/forms": [
        "jspm_packages/npm/@angular/forms@0.1.1"
      ],
      "@angular/router": [
        "jspm_packages/npm/@angular/router@3.0.0-alpha.8"
      ],
      "@angular/*": [
        "jspm_packages/npm/@angular/*@2.0.0-rc.3"
      ],
      "redux-thunk": [
        "jspm_packages/npm/redux-thunk@2.1.0"
      ],
      "@angular/platform-browser/src/browser/browser_adapter": [
        "jspm_packages/npm/@angular/platform-browser@2.0.0-rc.3/src/browser/browser_adapter"
      ],
      "rxjs/*": [
        "jspm_packages/npm/rxjs@5.0.0-beta.6/*"
      ]
    }
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules",
    "/src/typings/app.d.ts"
  ],
  "exclude": [
    "src/public/world_data.ts",
    "temp",
    "node_modules",
    "node_modules/*",
    "jspm_packages",
    "typings",
    "dist",
    "typings/*",
    "typings/main.d.ts",
    "typings/main"
  ],
  "compileOnSave": false,
  "buildOnSave": true
}